{
  "name": "fidlezer",
  "version": "0.0.1",
  "description": "Tests runner",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --no-cache --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
